# Automatically generated file by kdevops {{ kdevops_version }}
#
# Processed by start-mirroring.py.
#
# Keep in alphabetical order using the short_name as index, this order is
# only important so we know where to add new entries. Mirror references are
# dealt with through a second iteration by start-mirroring.py on this list.
# Only references with two levels are supported.
mirrors:
  - short_name: "blktrace"
    url: "{{ defaults_blktrace_git }}"
    target: "blktrace.git"
  - short_name: "dbench"
    url: "{{ defaults_dbench_git }}"
    target: "dbench.git"
  - short_name: "git"
    url: "{{ defaults_git_git }}"
    target: "git.git"
  - short_name: "kdevops"
    url: "{{ mirror_kdevops_url }}"
    target: "kdevops.git"
    refresh: "5m"
    refresh_on_boot: "10m"
  - short_name: "kdevops-results-archive"
    url: "{{ defaults_kdevops_results_archive_git }}"
    target: "kdevops-results-archive.git"
  - short_name: "linux"
    url: "{{ mirror_torvalds_url }}"
    target: "linux.git"
    refresh: "5m"
    refresh_on_boot: "10m"
  - short_name: "ltp"
    url: "{{ defaults_ltp_git }}"
    target: "ltp.git"
  - short_name: "nfstest"
    url: "{{ defaults_nfstest_git }}"
    target: "nfstest.git"
  - short_name: "mcgrof-linus"
    url: "{{ mirror_mcgrof_linus_url }}"
    target: "mcgrof-linus.git"
    refresh: "360m"
    refresh_on_boot: "10m"
    reference: "linux.git"
  - short_name: "stable"
    url: "{{ mirror_stable_url }}"
    target: "linux-stable.git"
    refresh: "120m"
    refresh_on_boot: "20m"
    reference: "linux.git"
  - short_name: "xfsprogs"
    url: "{{ defaults_xfsprogs_git }}"
    target: "xfsprogs-dev.git"
  - short_name: "xfsdump"
    url: "{{ defaults_xfsdump_git }}"
    target: "xfsdump-dev.git"
